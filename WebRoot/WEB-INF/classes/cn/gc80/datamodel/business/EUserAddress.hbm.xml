<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.gc80.datamodel.business.EUserAddress" table="LRN_USER_ADDRESS">
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="userId" type="java.lang.String">
            <column name="USER_ID" length="32" not-null="true" />
        </property>
        <property name="address" type="java.lang.String">
            <column name="ADDRESS" length="100" />
        </property>
        <property name="phone" type="java.lang.String">
            <column name="PHONE" length="11" />
        </property>
        <property name="isDefault" type="java.lang.String">
            <column name="ISDEFAULT" length="2" not-null="true" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="25" not-null="true" />
        </property>
        <property name="postcode" type="java.lang.String">
            <column name="POSTCODE" length="6" />
        </property>
        <property name="area" type="java.lang.String">
            <column name="AREA" length="10" />
        </property>
        <property name="addressName" type="java.lang.String">
            <column name="ADDRESS_NAME" length="32" />
        </property>
         <property name="orgId" type="java.lang.String">
            <column name="ORG_ID" length="32"/>
        </property>
         <property name="delflag" type="java.lang.String">
            <column name="DELFLAG" length="2" />
        </property>
    </class>
</hibernate-mapping>
