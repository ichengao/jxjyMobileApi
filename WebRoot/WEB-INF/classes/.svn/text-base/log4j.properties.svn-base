log4j.appender.R=org.apache.log4j.RollingFileAppender
log4j.appender.R.File=${catalina.home}/logs/training.log
log4j.appender.R.MaxBackupIndex=3
log4j.appender.R.MaxFileSize=10000KB
log4j.appender.R.layout=org.apache.log4j.PatternLayout
log4j.appender.R.layout.ConversionPattern=%d- %p [%t] %c (%M:%L) - %m%n
log4j.appender.log=org.apache.log4j.DailyRollingFileAppender
log4j.appender.log.layout=org.apache.log4j.PatternLayout
log4j.appender.log.layout.ConversionPattern=%d- %p [%t] %c (%M:%L) - %m%n
log4j.appender.out=org.apache.log4j.ConsoleAppender
log4j.appender.out.layout=org.apache.log4j.PatternLayout
log4j.appender.out.layout.ConversionPattern=%d- %p [%t] %c (%M\:%L) - %m%n 
log4j.logger.org.hibernate=error
log4j.logger.org.spring=error
log4j.logger.com.danga=error
log4j.logger.cn.cm=debug
log4j.rootLogger=error,R
log4j.appender.log.DatePattern=yyyy-MM-dd
